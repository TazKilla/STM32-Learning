#include <stdint.h>
#include <assert.h>

#include "../include/gpio_ports.h"

int activatePort();

int setPin(GPIOPort * port, int pin)
{
	*port->pPortOutDataReg |= 1 << pin;
	return 0;
}

int clearPin(GPIOPort * port, int pin)
{
	*port->pPortOutDataReg &= ~(1 << pin);
	return 0;
}
