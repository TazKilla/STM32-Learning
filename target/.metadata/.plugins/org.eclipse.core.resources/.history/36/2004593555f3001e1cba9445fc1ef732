#include <stdint.h>
#include <../../STM32F30xx/gpio_ports.h>

int main(void)
{
	uint64_t tics_cntr = 0;
	uint64_t toggle_cntr = 0;

	*GPIOPortB.pRCCReg |= 1 << 18;
	*GPIOPortB.pPortModeReg &= ~(3 << 26);
	*GPIOPortB.pPortModeReg |= 1 << 26;
	*GPIOPortB.pPortOutDataReg |= 1 << 13;

    /* Loop forever */
	for(;;)
	{
		if(tics_cntr - toggle_cntr >= 100)
		{
			*GPIOPortB.pPortOutDataReg |= 0 << 13;
			tics_cntr = toggle_cntr;
		}
		toggle_cntr++;
	}
}
